            },
            {
                "name": "eth1",
                "vrf": "default",
                "description": "marketing"
            }
        ]
    },
    "frr-ripd:ripd": {
        "instance": [
            {
                "vrf": "default",
                "allow-ecmp": "true",
                "distance": {
